{
    "_comment": "Copier vers Settings.json et adapter les valeurs selon environnement",
    "_version": "1.1.0",

    "Application": {
        "Name": "Get-ExchangeDelegation",
        "Environment": "PROD",
        "LogLevel": "Info"
    },

    "Paths": {
        "Logs": "./Logs",
        "Output": "./Output",
        "Checkpoints": "./Checkpoints"
    },

    "Retention": {
        "LogDays": 30,
        "OutputDays": 7,
        "CheckpointHours": 24
    },

    "Checkpoint": {
        "Enabled": true,
        "Interval": 50,
        "KeyProperty": "ExchangeGuid"
    }
}
